{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "()": "ragtime.core.logging.ColoredFormatter",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "default": {
      "formatter": "default",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "uvicorn": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    },
    "uvicorn.error": {
      "level": "INFO",
      "handlers": [
        "default"
      ],
      "propagate": false
    },
    "uvicorn.access": {
      "handlers": [
        "default"
      ],
      "level": "INFO",
      "propagate": false
    }
  }
}
